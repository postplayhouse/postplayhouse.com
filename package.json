{
	"name": "postplayhouse.com",
	"description": "postplayhouse.com",
	"version": "0.0.1",
	"scripts": {
		"clean": "rm -rf ./static/g",
		"copyFolder": "./scripts/copy_folder.cjs",
		"useCache": "npm run copyFolder -- --merge /opt/build/cache/hc_my_cache/static/g --to static/g",
		"fillCache": "npm run copyFolder -- --save static/g --to /opt/build/cache/hc_my_cache/static/g",
		"netlify": "npm run useCache && npm run build && npm run fillCache",
		"dev": "vite dev --port 3000",
		"build": "vite build",
		"preview": "vite preview --port 3000",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"test": "jest",
		"format": "prettier --write .",
		"prepare": "husky"
	},
	"lint-staged": {
		"*": [
			"prettier --ignore-unknown --write"
		]
	},
	"dependencies": {
		"compression": "^1.7.4",
		"js-yaml": "^4.1.0",
		"lodash-es": "^4.17.21",
		"marked": "^4.1.1"
	},
	"devDependencies": {
		"@popperjs/core": "^2.9.2",
		"@rollup/plugin-replace": "^5.0.5",
		"@sveltejs/adapter-netlify": "^3.0.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@testing-library/jest-dom": "^6.1.4",
		"@testing-library/svelte": "^4.1.0",
		"@types/eslint": "8.56.0",
		"@types/jest": "^29.5.8",
		"@types/lodash-es": "^4.14.186",
		"@types/marked": "^4.0.7",
		"@typescript-eslint/eslint-plugin": "^6.10.0",
		"@typescript-eslint/parser": "^6.10.0",
		"autoprefixer": "^10.4.16",
		"cssnano": "^6.0.1",
		"date-fns": "^2.23.0",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.35.1",
		"frontmatter": "^0.0.3",
		"husky": "^9.0.10",
		"jest": "^29.7.0",
		"jest-environment-jsdom": "^29.7.0",
		"lint-staged": "^15.2.1",
		"mdsvex": "^0.11.0",
		"postcss": "^8.4.21",
		"postcss-load-config": "^4.0.1",
		"prettier": "^3.2.0",
		"prettier-plugin-svelte": "^3.1.2",
		"svelte": "^4.2.9",
		"svelte-check": "^3.6.3",
		"svelte-image": "happycollision/svelte-image#6ba341afddbc3a34059d447e38f608e7dd29f1d3",
		"svelte-jester": "^3.0.0",
		"svelte-preprocess": "^5.1.3",
		"tailwindcss": "^3.1.8",
		"ts-jest": "^29.1.1",
		"tslib": "^2.6.2",
		"typescript": "5.2.2",
		"vite": "^5.0.3",
		"vite-plugin-watch-and-run": "^1.6.0"
	},
	"type": "module"
}
