{
  "name": "postplayhouse.com",
  "description": "postplayhouse.com",
  "version": "0.0.1",
  "scripts": {
    "clean": "rm -rf ./static/g && yarn transpile --clean",
    "dev": "yarn clean && yarn transpile && sapper dev",
    "transpile": "ts-node src/data/_helpers/build-svelte-templates.ts",
    "build": "yarn clean && sapper build --legacy",
    "export": "yarn clean && sapper export --legacy",
    "start": "node __sapper__/build",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test": "run-p --race dev cy:run"
  },
  "dependencies": {
    "compression": "^1.7.1",
    "js-yaml": "^3.13.1",
    "lodash": "^4.17.15",
    "polka": "next",
    "sirv": "^0.4.0",
    "svelte-image": "0.0.12"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "@types/compression": "^1.0.0",
    "@types/lodash": "^4.14.136",
    "@types/markdown-it": "^0.0.8",
    "@types/node": "^12.7.1",
    "babel-eslint": "^10.0.2",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-svelte3": "^2.7.3",
