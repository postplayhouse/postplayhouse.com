{
  "name": "postplayhouse.com",
  "description": "postplayhouse.com",
  "version": "0.0.1",
  "scripts": {
    "clean": "rm -rf ./static/g",
    "copyFolder": "./scripts/copy_folder.cjs",
    "useCache": "npm run copyFolder -- --merge /opt/build/cache/hc_my_cache/static/g --to static/g",
    "fillCache": "npm run copyFolder -- --save static/g --to /opt/build/cache/hc_my_cache/static/g",
    "netlify": "npm run useCache && npm run build && npm run fillCache",
    "dev": "vite dev --port 3000",
    "build": "vite build",
    "preview": "vite preview --port 3000",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check --plugin-search-dir=. . && eslint .",
    "test": "jest",
    "format": "prettier --write --plugin-search-dir=. ."
  },
  "dependencies": {
    "@sveltejs/kit": "^1.27.3",
    "compression": "^1.7.4",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "marked": "^4.1.1"
  },
  "devDependencies": {
    "@popperjs/core": "^2.9.2",
    "@rollup/plugin-replace": "^5.0.2",
    "@sveltejs/adapter-netlify": "^2.0.8",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/svelte": "^4.0.5",
    "@types/jest": "^29.5.8",
    "@types/lodash": "^4.14.186",
    "@types/marked": "^4.0.7",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "autoprefixer": "^10.4.16",
    "cssnano": "^6.0.1",
    "date-fns": "^2.23.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-svelte": "^2.35.0",
    "frontmatter": "^0.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "mdsvex": "^0.11.0",
    "postcss": "^8.4.21",
    "postcss-load-config": "^4.0.1",
    "prettier": "^3.0.3",
    "prettier-plugin-svelte": "^3.0.3",
    "svelte": "^4.2.2",
    "svelte-check": "^3.5.2",
    "svelte-image": "happycollision/svelte-image#6ba341afddbc3a34059d447e38f608e7dd29f1d3",
    "svelte-jester": "^3.0.0",
    "svelte-preprocess": "^5.0.4",
    "tailwindcss": "^3.1.8",
    "ts-jest": "^29.1.1",
    "tslib": "^2.6.2",
    "typescript": "5.2.2",
    "vite-plugin-watch-and-run": "^1.4.1"
  },
  "type": "module"
}
