{
  "name": "postplayhouse.com",
  "description": "postplayhouse.com",
  "version": "0.0.1",
  "scripts": {
    "clean": "rm -rf ./static/g",
    "copyFolder": "./scripts/copy_folder.cjs",
    "useCache": "npm run copyFolder -- --merge /opt/build/cache/hc_my_cache/static/g --to static/g",
    "fillCache": "npm run copyFolder -- --save static/g --to /opt/build/cache/hc_my_cache/static/g",
    "netlify": "npm run useCache && npm run build && npm run fillCache",
    "dev": "vite dev --port 3000",
    "build": "vite build",
    "preview": "vite preview --port 3000",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check --plugin-search-dir=. . && eslint .",
    "test": "jest",
    "format": "prettier --write --plugin-search-dir=. ."
  },
  "dependencies": {
    "@sveltejs/kit": "^1.0.0-next.508",
    "compression": "^1.7.4",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "marked": "^4.1.1"
  },
  "devDependencies": {
    "@rollup/plugin-replace": "^4.0.0",
    "@sveltejs/adapter-static": "1.0.0-next.44",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/svelte": "^3.2.1",
    "@types/jest": "^27.0.0",
    "@types/lodash": "^4.14.186",
    "@types/marked": "^4.0.7",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "autoprefixer": "^10.4.12",
    "cssnano": "^5.1.13",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "frontmatter": "^0.0.3",
    "jest": "^29.1.2",
    "jest-environment-jsdom": "^29.1.2",
    "mdsvex": "^0.10.6",
    "postcss": "^8.4.17",
    "postcss-load-config": "^4.0.1",
    "prettier": "^2.7.1",
    "prettier-plugin-svelte": "^2.7.1",
    "svelte": "^3.50.1",
    "svelte-check": "^2.9.1",
    "svelte-image": "happycollision/svelte-image#6ba341afddbc3a34059d447e38f608e7dd29f1d3",
    "svelte-jester": "^2.3.2",
    "svelte-preprocess": "^4.10.7",
    "tailwindcss": "^3.1.8",
    "ts-jest": "^29.0.3",
    "tslib": "^2.4.0",
    "typescript": "~4.8.4",
    "vite-plugin-watch-and-run": "^1.0.3"
  },
  "type": "module"
}
