{
	"name": "postplayhouse.com",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"gen:phrases": "node ./scripts/generate_passphrases",
		"clean": "rm -rf ./node_modules/.cache",
		"copyFolder": "./scripts/copy_folder.cjs",
		"useCache": "npm run copyFolder -- --merge /opt/build/cache/hc_my_cache/node_modules/.cache --to node_modules/.cache",
		"fillCache": "npm run copyFolder -- --save node_modules/.cache --to /opt/build/cache/hc_my_cache/node_modules/.cache",
		"netlify": "npm run useCache && npm run build && npm run fillCache",
		"dev": "vite dev --port 3000",
		"build": "vite build",
		"preview": "vite preview --port 3000",
		"test": "npm run test:integration && npm run test:unit",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"test:integration": "playwright test",
		"test:unit": "vitest",
		"prepare": "husky"
	},
	"lint-staged": {
		"*": [
			"prettier --ignore-unknown --write"
		]
	},
	"dependencies": {
		"@sentry/sveltekit": "^8.0.0",
		"compression": "^1.7.4",
		"date-fns": "^2.23.0",
		"js-yaml": "^4.1.0",
		"jsdom": "^24.0.0",
		"jszip": "^3.10.1",
		"lodash-es": "^4.17.21",
		"marked": "^4.1.1",
		"prosemirror-example-setup": "^1.2.2",
		"prosemirror-markdown": "^1.12.0",
		"prosemirror-state": "^1.4.3",
		"prosemirror-view": "^1.32.7"
	},
	"devDependencies": {
		"@octokit/rest": "^20.0.2",
		"@playwright/test": "^1.28.1",
		"@popperjs/core": "^2.9.2",
		"@rollup/plugin-replace": "^5.0.5",
		"@sveltejs/adapter-netlify": "^3.0.0",
		"@sveltejs/enhanced-img": "^0.3.9",
		"@sveltejs/kit": "^2.8.3",
		"@sveltejs/vite-plugin-svelte": "^4.0.0",
		"@tailwindcss/typography": "^0.5.10",
		"@types/eslint": "^8.56.7",
		"@types/js-yaml": "^4.0.9",
		"@types/jsdom": "^21.1.6",
		"@types/lodash-es": "^4.14.186",
		"@types/marked": "^4.0.7",
		"autoprefixer": "^10.4.16",
		"cssnano": "^6.0.1",
		"eslint": "^9.0.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.45.1",
		"globals": "^15.0.0",
		"gray-matter": "^4.0.3",
		"husky": "^9.0.10",
		"lint-staged": "^15.2.1",
		"mdsvex": "^0.11.0",
		"postcss": "^8.4.32",
		"postcss-load-config": "^5.0.2",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.2.6",
		"prettier-plugin-tailwindcss": "0.6.8",
		"random-words": "^2.0.1",
		"rehype-slug": "^6.0.0",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"tailwindcss": "^3.3.6",
		"tslib": "^2.4.1",
		"typescript": "^5.5.0",
		"typescript-eslint": "^7.5.0",
		"vite": "^5.4.4",
		"vite-plugin-watch-and-run": "^1.7.0",
		"vitest": "^1.2.0"
	},
	"type": "module",
	"pnpm": {
		"patchedDependencies": {
			"sharp@0.33.4": "patches/sharp@0.33.4.patch",
			"imagetools-core@7.0.1": "patches/imagetools-core@7.0.1.patch"
		}
	}
}
