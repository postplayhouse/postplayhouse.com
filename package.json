{
  "name": "postplayhouse.com",
  "description": "postplayhouse.com",
  "version": "0.0.1",
  "scripts": {
    "clean": "rm -rf ./static/g",
    "copyFolder": "./scripts/copy_folder.cjs",
    "useCache": "npm run copyFolder -- --merge /opt/build/cache/hc_my_cache/static/g --to static/g",
    "fillCache": "npm run copyFolder -- --save static/g --to /opt/build/cache/hc_my_cache/static/g",
    "netlify": "npm run useCache && npm run build:noClean && npm run fillCache",
    "dev": "svelte-kit dev",
    "build": "npm run clean && svelte-kit build",
    "build:noClean": "svelte-kit build",
    "package": "svelte-kit package",
    "preview": "svelte-kit preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check --plugin-search-dir=. . && eslint .",
    "format": "prettier --write --plugin-search-dir=. ."
  },
  "dependencies": {
    "compression": "^1.7.1",
    "js-yaml": "^3.13.1",
    "lodash": "^4.17.15",
    "marked": "^4.0.12"
  },
  "devDependencies": {
    "@kitql/vite-plugin-watch-and-run": "^0.3.0",
    "@rollup/plugin-replace": "^3.0.0",
    "@sveltejs/adapter-static": "next",
    "@sveltejs/kit": "next",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/svelte": "^3.0.3",
    "@types/jest": "^26.0.24",
    "@types/lodash": "^4.14.178",
    "@types/marked": "^4.0.2",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "autoprefixer": "^10.4.4",
    "cssnano": "^5.0.6",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "frontmatter": "^0.0.3",
    "jest": "^27.0.6",
    "markdown-it": "^12.3.2",
    "mdsvex": "^0.8.8",
    "postcss": "^8.4.12",
    "postcss-load-config": "^3.1.0",
    "prettier": "^2.5.1",
    "prettier-plugin-svelte": "^2.5.0",
    "svelte": "^3.44.0",
    "svelte-check": "^2.2.6",
    "svelte-image": "happycollision/svelte-image#6ba341afddbc3a34059d447e38f608e7dd29f1d3",
    "svelte-jester": "^2.3.2",
    "svelte-preprocess": "^4.10.1",
    "tailwindcss": "^3.1.4",
    "ts-jest": "^27.0.4",
    "tslib": "^2.3.1",
    "typescript": "~4.6.2"
  },
  "type": "module",
  "volta": {
    "node": "16.7.0",
    "npm": "7.18.1"
  }
}
